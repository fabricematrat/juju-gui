          "Optional": false,
          "Limit": 0,
          "Scope": "global"
        },
        "nrpe-external-master": {
          "Name": "nrpe-external-master",
          "Role": "provider",
          "Interface": "nrpe-external-master",
          "Optional": false,
          "Limit": 0,
          "Scope": "container"
        }
      },
      "Peers": {
